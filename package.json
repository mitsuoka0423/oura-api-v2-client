{
  "name": "oura-api-v2-client",
  "version": "0.1.7",
  "description": "API client for Oura API V2",
  "main": "dist/cjs/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "node": "./dist/esm/index.mjs",
    "default": "./dist/cjs/index.js"
  },
  "scripts": {
    "dev": "ts-node src/index.ts",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm run lint:fix && npm-run-all clean && npm run build:cjs && npm run build:esm",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "start": "node .",
    "check-types": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "lint": "npm-run-all eslint check-types",
    "lint:fix": "npm-run-all eslint:fix check-types format",
    "prepublish": "$(npm bin)/tsc"
  },
  "keywords": ["oura", "oura ring", "oura api", "oura api v2"],
  "author": "Takahiro MITSUOKA",
  "bugs": {
    "url" : "https://github.com/mitsuoka0423/oura-api-v2-client/issues",
    "email" : "mono0423@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mitsuoka0423/oura-api-v2-client"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.12.54",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "6.11.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.ts": "tsc --noEmit",
    "*.{js,ts,json}": "prettier --write"
  },
  "dependencies": {
    "axios": "^0.26.1"
  }
}
